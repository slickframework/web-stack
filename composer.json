{
  "name": "slick/web_stack",
  "description": "MVC implementation for Slick Framework.",
  "minimum-stability": "dev",
  "license": "MIT",
  "version": "v1.0.0-alpha",
  "keywords": [
    "slick",
    "PSR-7 stack"
  ],
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Slick\\WebStack\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Features\\App\\": "features/app/src"
    }
  },
  "require": {
    "php" : ">=5.6",
    "slick/di": "^2.2",
    "slick/http": "^1.2",
    "slick/configuration": "@dev",
    "symfony/yaml": "^3.2",
    "aura/router": "^3.0",
    "slick/template": "^1.2",
    "filp/whoops": "^2.1",
    "league/flysystem": "^1.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.2",
    "behat/mink-extension": "^2.2",
    "behat/mink-goutte-driver": "^1.2",
    "slick/console": "^1.2@dev"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
