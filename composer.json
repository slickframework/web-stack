{
  "name": "slick/webstack",
  "description": "PSR-15 HTTP middleware stack that can help you create web applications or web services for HTTP protocol.",
  "minimum-stability": "dev",
  "license": "MIT",
  "version": "v2.0.0",
  "keywords": [
    "slick",
    "PSR-15",
    "PSR-7",
    "http"
  ],
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Slick\\WebStack\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Slick\\WebStack\\": "tests/",
      "Features\\App\\": "features/app/src"
    }
  },
  "require": {
    "php": ">=8.2",
    "doctrine/collections": "^2.3",
    "doctrine/orm": "^3.2",
    "monolog/monolog": "3.x-dev",
    "psr/log": ">=2.0",
    "slick/configuration": "^2.0",
    "slick/di": "^2.0@dev",
    "slick/fswatch": "^0.1.0@dev",
    "slick/http": ">=3.0",
    "slick/module-api": ">=0.4",
    "symfony/console": "7.2.x-dev",
    "symfony/routing": "7.2.x-dev",
    "vlucas/phpdotenv": "^5.6@dev"
  },
  "require-dev": {
    "behat/behat": "^3.0@dev",
    "behat/mink": "^1.0@dev",
    "ediasoft/mink-selenium4-driver": "dev-master",
    "friends-of-behat/mink-extension": "^2.7",
    "phpmd/phpmd": "dev-master",
    "phpspec/prophecy": "^1.0@dev",
    "phpspec/prophecy-phpunit": "^2.0@dev",
    "phpstan/phpstan": "2.0.x-dev",
    "phpunit/phpunit": "^11.3@dev",
    "squizlabs/php_codesniffer": "4.0.x-dev"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.3-dev",
      "dev-develop": "2.0-dev"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
    "check-code": "phpmd --ignore-errors-on-exit src html build/phpmd.xml > build/phpmd.html && phpstan analyze"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
